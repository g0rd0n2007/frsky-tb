\begin_layout Subsubsection
CRC calculation
\end_layout

\begin_layout Standard
It is not a simple sumup.
 Below is correct method:
\end_layout

\begin_layout Quotation
uint8_t calcCrc(const uint8_t* buf,
 size_t len) {
\end_layout

\begin_deeper
\begin_layout Quotation
short crc = 0;
\end_layout

\begin_deeper
\begin_layout Quotation
for(int i=0;i<len;i++) {
\end_layout

\begin_deeper
\begin_layout Quotation
crc += buf[i];
 //0-1FF
\end_layout

\begin_layout Quotation
crc += crc >> 8;
 //0-100
\end_layout

\begin_layout Quotation
crc &= 0x00ff;
\end_layout

\begin_layout Quotation
crc += crc >> 8;
 //0-0FF
\end_layout

\begin_layout Quotation
crc &= 0x00ff;
\end_layout

\begin_layout Quotation
}
\end_layout

\end_deeper
\begin_layout Quotation
return ~crc;
\end_layout

\end_deeper
\begin_layout Quotation
}
\end_layout

\end_deeper
\end_deeper
\end_deeper
